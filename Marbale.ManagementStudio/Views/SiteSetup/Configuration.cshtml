@{
    ViewBag.Title = "Configuration";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="config">
    <div class="panel panel-default" style="width: 1062px; padding: 41px; margin: 11px; height: 800px; background-color: darkcyan;">
        <div id="Tabs" role="tabpanel">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs" role="tablist">
                <li class="active"><a onclick="getValues()" aria-controls="personal" role="tab" data-toggle="tab">Values</a></li>
                <li><a aria-controls="employment" onclick="getSettings()" role="tab" data-toggle="tab">Settings</a></li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content" style="padding-top: 20px">
                <div role="tabpanel" class="tab-pane active" id="container">
                </div>
                <div role="tabpanel" class="tab-pane">
                </div>
            </div>
        </div>
    </div>
    <div id="container1"></div>

</div>

<script>
    $(document).ready(function () {
        $('#loading').show();
        getValues();

    })
    function getValues() {
        $('#loading').show();
        $('#container').load('/SiteSetup/Values');
    }
    function getSettings() {
        debugger
        $('#loading').fadeIn();
        $('#container').load('/SiteSetup/Settings');
        $('#loading').fadeOut();

    }
</script>
